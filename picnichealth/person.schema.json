{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "personId",
      "name",
      "email",
      "dob"
    ],
    "properties": {
      "personId": {
        "$id": "#/items/properties/personId",
        "type": "string",
        "title": "The Personid Schema",
        "default": "",
        "examples": [
          "81163710-c2a2-4750-8e7f-8c7e18575695"
        ],
        "pattern": "^(.*)$"
      },
      "name": {
        "$id": "#/items/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "John Smith"
        ],
        "pattern": "^(.*)$"
      },
      "email": {
        "$id": "#/items/properties/email",
        "type": "string",
        "title": "The Email Schema",
        "default": "",
        "examples": [
          "jsmith@example.com"
        ],
        "pattern": "^(.*)$"
      },
      "dob": {
        "$id": "#/items/properties/dob",
        "type": "string",
        "title": "The Dob Schema",
        "default": "",
        "examples": [
          "1978-07-23T12:00:00"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}