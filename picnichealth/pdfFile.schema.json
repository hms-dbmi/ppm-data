{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "fileId",
      "personId",
      "fileType"
    ],
    "properties": {
      "fileId": {
        "$id": "#/items/properties/fileId",
        "type": "string",
        "title": "The Fileid Schema",
        "default": "",
        "examples": [
          "1a48f53f-4b7a-4c8f-acbd-1e031fa08bbc"
        ],
        "pattern": "^(.*)$"
      },
      "personId": {
        "$id": "#/items/properties/personId",
        "type": "string",
        "title": "The Personid Schema",
        "default": "",
        "examples": [
          "81163710-c2a2-4750-8e7f-8c7e18575695"
        ],
        "pattern": "^(.*)$"
      },
      "fileType": {
        "$id": "#/items/properties/fileType",
        "type": "string",
        "title": "The Filetype Schema",
        "default": "",
        "examples": [
          "pdf_files"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}